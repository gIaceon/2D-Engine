{
  "name": "2D-Engine",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Common": {
        "$path": "src/shared"
      },
      "Packages": {
        "$path": "Packages"
      },
      "Controllers": {
        "$path": "Controllers"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "$properties": {
        "EnableMouseLockOption": false
      },
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    }
  }
}